{
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.rulers": [80, 100],
  "editor.scrollBeyondLastLine": false,
  "editor.semanticTokenColorCustomizations": {
    "[Noctis Sereno]": {
      "enabled": true,
      "rules": {
        "namespace": "#49e9c9",
        "variable": "#e2c9aa"
      }
    }
  },
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "onlySnippets",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 7,
  "editor.fontFamily": "NerdCodeProPlus",
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,

  "explorer.confirmDelete": false,
  "explorer.sortOrderLexicographicOptions": "unicode",

  "files.hotExit": "off",
  "files.insertFinalNewline": true,

  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.exclude": {
    "**/third_party": true,
    "**/3rd_party": true,
    "**/vendor": true
  },

  "git.autofetch": true,
  "git.confirmSync": false,
  "git.detectSubmodules": false,
  "git.ignoreRebaseWarning": true,

  "workbench.startupEditor": "none",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorCustomizations": {
    "statusBar.foreground": "#b9d3d6",
    "statusBar.background": "#005f5f",
    "statusBar.noFolderForeground": "#b9d3d6",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingForeground": "#b9d3d6",
    "statusBar.debuggingBackground": "#005f2e",
    "statusBarItem.hoverBackground": "#007d7d",
    "activityBar.inactiveForeground": "#169fb199",
    "editorWidget.background": "#083d44dd",
    "editor.findMatchBackground": "#40bf6a99",
    "editor.findMatchHighlightBackground": "#407dbf99"
  },
  "workbench.colorTheme": "Noctis Sereno",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.revealIfOpen": true,

  "window.commandCenter": true,
  "window.enableMenuBarMnemonics": false,

  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.mouseWheelScrollSensitivity": 0.2,

  "problems.showCurrentInStatus": true,

  "security.workspace.trust.enabled": false,

